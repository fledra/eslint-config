{
  "name": "@fledra/eslint-config",
  "description": "One opinionated config for ESLint",
  "author": "Baran D. <baran@fledra.dev> (https://fledra.dev)",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.7.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fledra/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/fledra/eslint-config/issues"
  },
  "scripts": {
    "prepare": "husky",
    "build": "pnpm generate && tsup --clean --dts",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "test": "vitest --run",
    "test:dev": "vitest",
    "generate": "tsx scripts/typegen.ts",
    "prepack": "pnpm build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-import-x": "^4.10.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "find-up-simple": "^1.0.1",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.2",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@typescript-eslint/typescript-estree": "^8.28.0",
    "@typescript-eslint/utils": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-typegen": "^2.1.0",
    "eslint-vitest-rule-tester": "^2.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  }
}
